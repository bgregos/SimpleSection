<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="405.0" prefWidth="592.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapp.SettingsTab">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Settings">
         <font>
            <Font name="Ubuntu" size="26.0" />
         </font>
      </Label>
      <ComboBox fx:id="term" layoutX="14.0" layoutY="81.0" onMouseClicked="#updateTerm" prefHeight="26.0" prefWidth="156.0" />
      <Label layoutX="14.0" layoutY="65.0" text="Term" />
      <Button fx:id="debug" layoutX="135.0" layoutY="16.0" mnemonicParsing="false" onMouseClicked="#saveSettings" text="Save Settings" />
   </children>
</AnchorPane>
