<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="600.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapp.OverviewController">
	<children>
		<TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<tabs>
				<Tab text="Section Listings">
					<content>
						<fx:include fx:id="MyListTab" source="SectionList.fxml" />
					</content>
				</Tab>
				<Tab onSelectionChanged="#onClickMyClasses" text="My Classes">
					<content>
						<fx:include fx:id="myClassList" source="MyClassList.fxml" />
					</content>
				</Tab>
				<Tab onSelectionChanged="#onClickSchedule" text="Schedule">
					<content>
						<fx:include fx:id="scheduleTab" source="Schedule.fxml" />
					</content>
				</Tab>
            <Tab text="Professor Ratings">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <WebView fx:id="webview" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                        <Button fx:id="forwardButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onForward" text="Forward" />
                        <Button fx:id="backButton" layoutX="25.0" layoutY="40.0" mnemonicParsing="false" onAction="#onBack" text="Back" />
                        <Button fx:id="koofers" layoutX="16.0" layoutY="80.0" mnemonicParsing="false" onAction="#onKoofers" text="Koofers" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
				<Tab text="About">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
							<children>
								<TextArea fx:id="aboutText" editable="false" prefHeight="558.0" prefWidth="862.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
							</children>
						</AnchorPane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</children>
</AnchorPane>
